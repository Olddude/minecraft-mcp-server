{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "bashdb",
      "request": "launch",
      "name": "[Debug] Shell",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/scripts/command.sh",
      "args": ["time set day"],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "[Debug] Server",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
